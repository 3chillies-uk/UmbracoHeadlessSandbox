{
	"$schema": "appsettings-schema.json",
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"Microsoft.Hosting.Lifetime": "Information",
				"System": "Warning"
			}
		}
	},
	"HeadlessPreview": {
		"TemplateUrl": "https://localhost:4321/{slug}"
	},
	"Umbraco": {
		"DeliveryApiExtensions": {
			"Preview": {
				"Enabled": true,
				"Media": {
					"Enabled": true
				},
				"AllowedUserGroupAliases": [],
				"ContentAppWeight": -50
			},
			"TypedSwagger": {
				"Enabled": true,
				"Mode": "Auto"
			}
		},
		"CMS": {
			"WebRouting": {
				"DisableRedirectUrlTracking": true,
				"UmbracoApplicationUrl": "https://localhost:44335/"
			},
			"Unattended": {
				"UpgradeUnattended": true
			},
			"Runtime": {
				"MaxRequestLength": 51200
			},
			"Hosting": {
				"Debug": false
			},
			"DeliveryApi": {
				"Enabled": true,
				"PublicAccess": false,
				"ApiKey": "6536e4ec-cba3-4411-a21e-d933eb7dcc23",
				"DisallowedContentTypeAliases": [],
				"RichTextOutputAsJson": false,
				"Media": {
					"Enabled": true,
					"PublicAccess": false
				},
				"OutputCache": {
					"Enabled": false,
					"ContentDuration": "00:01:00",
					"MediaDuration": "01:00:00"
				}
			},
			"Global": {
				"Id": "6c06745b-5f80-477e-a500-f7a11af8da5f",
				"SanitizeTinyMce": true,
				"DefaultUILanguage": "en-us",
				"HideTopLevelNodeFromPath": true,
				"UmbracoPath": "~/umbraco",
				"UseHttps": true,
				"ReservedUrls": "~/.well-known",
				"ReservedPaths": "~/app_plugins/,~/install/,~/mini-profiler-resources/,~/umbraco/,~/@vite/,~/@id/",
				"TimeOut": "00:20:00",
				"VersionCheckPeriod": 0,
				"IconsPath": "~/umbraco/assets/icons",
				"UmbracoCssPath": "~/css",
				"UmbracoMediaPath": "~/media",
				"InstallMissingDatabase": false,
				"DisableElectionForSingleServer": true,
				"DatabaseFactoryServerVersion": "SqlServer.V2019",
				"NoNodesViewPath": "~/umbraco/UmbracoWebsite/NoNodes.cshtml",
				"SqlWriteLockTimeOut": "00:00:05"
			},
			"ModelsBuilder": {
				"ModelsMode": "SourceCodeAuto",
				"ModelsDirectory": "~/Models/DocumentTypes",
				"ModelsNamespace": "UmbracoHeadlessSandbox.Umbraco.Models.DocumentTypes",
				"AcceptUnsafeModelsDirectory": true
			},
			"Content": {
				"MacroErrors": "Throw",
				"AllowEditInvariantFromNonDefault": true,
				"ContentVersionCleanupPolicy": {
					"EnableCleanup": true
				}
			},
			"Tours": {
				"EnableTours": false
			},
			"Security": {
				"AllowConcurrentLogins": false,
				"KeepUserLoggedIn": false,
				"UsernameIsEmail": true,
				"AllowPasswordReset": false,
				"HideDisabledUsersInBackoffice": false,
				"UserPassword": {
					"RequiredLength": 10,
					"RequireNonLetterOrDigit": true,
					"RequireDigit": true,
					"RequireLowercase": true,
					"RequireUppercase": true,
					"MaxFailedAccessAttemptsBeforeLockout": 5
				},
				"MemberPassword": {
					"RequiredLength": 10,
					"RequireNonLetterOrDigit": true,
					"RequireDigit": true,
					"RequireLowercase": true,
					"RequireUppercase": true,
					"MaxFailedAccessAttemptsBeforeLockout": 5
				}
			}
		}
	},
	"ConnectionStrings": {
		"umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
		"umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
	},

	"uSync": {
		"ForceFips": true,
		"Settings": {
			"RootFolder": "uSync/v13/",
			"DefaultSet": "Default",
			"ImportAtStartup": "None",
			"ExportAtStartup": "None",
			"ExportOnSave": "All",
			"UiEnabledGroups": "All",
			"ReportDebug": false,
			"AddOnPing": true,
			"FailOnMissingParent": false,
			"CacheFolderKeys": true,
			"ShowVersionCheckWarning": true,
			"CustomMappings": {},
			"EnableHistory": true
		},
		"Sets": {
			"Default": {
				"Enabled": true,
				"HandlerGroups": [],
				"DisabledHandlers": [
					"contentHandler",
					"mediaHandler",
					"domainHandler",
					"languageHandler",
					"contentTemplateHandler",
					"relationTypeHandler",
					"memberHandler",
					"userHandler",
					"vendrPaymentMethodHandler"
				],
				"HandlerDefaults": {
					"Enabled": true,
					"Actions": [],
					"UseFlatStructure": false,
					"GuidNames": false,
					"FailOnMissingParent": false,
					"Group": "",
					"Settings": {}
				},
				"Handlers": {
					"DictionaryHandler": {
						"Settings": {
							"CreateOnly": "true"
						}
					},
					"UserGroupHandler": {
						"Settings": {
							"CreateOnly": "true"
						}
					},
					"UserHandler": {
						"Settings": {
							"CreateOnly": "true"
						}
					},
					"MemberTypeHandler": {
						"Settings": {
							"CreateOnly": "true"
						}
					},
					"MemberGroupHandler": {
						"Settings": {
							"CreateOnly": "true"
						}
					},
					"MemberHandler": {
						"Settings": {
							"CreateOnly": "true"
						}
					}
				}
			}
		}
	}
}
